function _init()
--player 
 p_x = 64
 p_y = 64
 p_spd = 2
 p_spr = 1
--item
 i.x = nil
 i.y = nil 
 
 a = 0
 
 
 map(0,0,0,0,128,128)

end


-->8
function _draw()
 --pset(x,y)
 --print("☉",x,y,x)
 --cls()
 --circ(x,y,a,rnd(16))
 spr(p_spr,p_x,p_x)
 spr(i_spr,rnd(100),rnd(100))
 

end



-->8
function _update()

--move player
 if (btn(➡️)) p_x += x +1
 if (btn(⬇️)) p_y += y +1
 if (btn(⬅️)) p_x -= x -1
 if (btn(⬇️)) p_y -= y -1
 
 if (btn(4)) a = a-1
 if (btn(5)) a = a+1
 
 
 
end
